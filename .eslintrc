{
  "env": {
    "jest": true
  },
  "extends": [
    "plugin:jsdoc/recommended",
    "tjw-base",
    "tjw-jest",
    "tjw-vue"
  ],
  "plugins": [
    "jsdoc"
  ],
  "rules": {
    "vue/multi-word-component-names": "off"
  },
  "settings": {
    "jsdoc": {
      "tagNamePreference": {
        "returns": "return"
      }
    }
  }
}
